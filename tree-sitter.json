{
  "grammars": [
    {
      "name": "ppr",
      "camelcase": "PPR",
      "scope": "source.ppr",
      "file-types": [
        ".ppr"
      ],
      "injection-regex": "^ppr$"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "parser for paper—ppr—projects",
    "authors": [
      {
        "name": "Adamina Barx",
        "email": "adaminabarx@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-ppr"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  },
  "tree-sitter": [
    {
      "scope": "source.markdownsimple",
      "file-types": [
        "mds"
      ],
      "highlights": [
        "queries/highlights.scm"
      ]
    }
  ]
}
