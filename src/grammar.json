{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "ppr",
  "rules": {
    "document": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "function"
          },
          {
            "type": "SYMBOL",
            "name": "title"
          },
          {
            "type": "SYMBOL",
            "name": "heading"
          },
          {
            "type": "SYMBOL",
