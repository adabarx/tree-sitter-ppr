          }
        ]
      }
    },
    "title": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\\"
        },
        {
          "type": "STRING",
          "value": "T"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "sentence"
              }
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "fragment"
                },
                {
                  "type": "BLANK"
                }
              ]
            }
          ]
        }
      ]
    },
    "heading": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\\"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "h1"
            },
            {
              "type": "SYMBOL",
              "name": "h2"
            },
            {
              "type": "SYMBOL",
              "name": "h3"
            },
            {
              "type": "SYMBOL",
              "name": "h4"
            },
            {
              "type": "SYMBOL",
              "name": "h5"
            },
            {
              "type": "SYMBOL",
              "name": "h6"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "sentence"
              }
            },
            {
              "type": "CHOICE",
              "members": [
