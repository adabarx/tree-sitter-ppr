                  "type": "SYMBOL",
                  "name": "fragment"
                },
                {
                  "type": "BLANK"
                }
              ]
            }
          ]
        }
      ]
    },
    "bookmark": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\\"
        },
        {
          "type": "STRING",
          "value": "B"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "sentence"
              }
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "fragment"
                },
                {
                  "type": "BLANK"
                }
              ]
            }
          ]
        }
      ]
    },
    "paragraph": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\\"
        },
        {
          "type": "STRING",
          "value": "P"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "sentence"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "fragment"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "identifier": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "[a-z]"
        },
        {
          "type": "PATTERN",
          "value": "[a-zA-Z\\d]*"
        }
      ]
    },
    "parameter": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "[a-z]"
        },
        {
          "type": "PATTERN",
          "value": "[a-zA-Z\\d]*"
        }
      ]
    },
    "value": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "PATTERN",
          "value": "[^\"]+"
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "function": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\\"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "parameter"
              },
              {
                "type": "STRING",
                "value": ":"
              },
              {
                "type": "SYMBOL",
                "name": "value"
              },
              {
                "type": "STRING",
                "value": ","
              }
            ]
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "parameter"
                },
                {
                  "type": "STRING",
                  "value": ":"
                },
                {
                  "type": "SYMBOL",
                  "name": "value"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "word": {
      "type": "PATTERN",
      "value": "[a-zA-Z'\\d]+"
    },
    "hyphen": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "word"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "-"
              },
              {
                "type": "SYMBOL",
                "name": "word"
              }
            ]
          }
        }
      ]
    },
    "style_marker": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "bold"
        },
        {
          "type": "SYMBOL",
          "name": "italics"
        },
        {
          "type": "SYMBOL",
          "name": "underline"
        },
        {
          "type": "SYMBOL",
          "name": "reference"
        }
      ]
    },
    "sentence": {
      "type": "PREC",
      "value": 3,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "REPEAT1",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "hyphen"
                },
                {
                  "type": "SYMBOL",
                  "name": "word"
                },
                {
                  "type": "SYMBOL",
                  "name": "punctuation"
                },
                {
                  "type": "SYMBOL",
                  "name": "_surround"
                },
                {
                  "type": "SYMBOL",
                  "name": "style_marker"
                },
                {
                  "type": "SYMBOL",
                  "name": "other"
                }
              ]
            }
          },
          {
            "type": "SYMBOL",
            "name": "end"
          }
        ]
      }
    },
    "fragment": {
      "type": "PREC",
      "value": 2,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "hyphen"
            },
            {
              "type": "SYMBOL",
              "name": "word"
            },
            {
              "type": "SYMBOL",
              "name": "punctuation"
            },
            {
              "type": "SYMBOL",
              "name": "_surround"
            },
            {
              "type": "SYMBOL",
              "name": "style_marker"
            },
            {
              "type": "SYMBOL",
              "name": "other"
            }
          ]
        }
      }
    },
    "end": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "period"
        },
        {
          "type": "SYMBOL",
          "name": "question_mark"
        },
        {
          "type": "SYMBOL",
          "name": "exclamation_point"
        }
      ]
    },
    "punctuation": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "comma"
        },
        {
          "type": "SYMBOL",
          "name": "semicolon"
        },
        {
          "type": "SYMBOL",
          "name": "colon"
        },
        {
          "type": "SYMBOL",
          "name": "fslash"
        },
        {
          "type": "SYMBOL",
          "name": "emdash"
        }
      ]
    },
    "_surround": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "quote"
        },
        {
          "type": "SYMBOL",
          "name": "parentheses"
        }
      ]
    },
    "quote": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "\""
